<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="viewModel"
            type="com.bluelinelabs.conductor.demo.controllers.BindedSharedModel" />
    </data>

    <FrameLayout
        android:id="@+id/fl_src"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/text_view_0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="@{() -> viewModel.goToEndController()}"
            android:text="@string/app_name"
            android:textAllCaps="false"
            android:transitionName="@string/transition_tag_title" />

    </FrameLayout>

</layout>